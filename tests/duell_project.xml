<?xml version="1.0" encoding="utf-8"?>
<project>

	<app title="Filesystem Testing"
		 file="filesystemtests"
		 package="com.haxe.duell.filesystemtesting"
		 version="0.0.1"
		 company="GameDuell GmbH" />

	<main name="MainTester" />

	<source path="Source" />

	<duelllib name="duellkit" version="6.0.0+" />
	<duelllib name="unittest" version="5.2.1+"/>
	<duelllib name="filesystem" version="electron_custom_mainProcessFile"/>
	<duelllib name="electron" version="master" if="electron" />

	<platform-config>
		<electron>
			<main source="ElectronTestMain" targetName="main" />
		</electron>
	</platform-config>

	<library-config>
		<filesystem>
			<static-assets path="Assets" />
			<ignore regex=" ~/.*\.tobeignored.*/i" />
		</filesystem>
	</library-config>

</project>
